{
  "name": "tablekit",
  "description": "TableCheck's Design System",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "repository": "github:tablecheck/tablekit",
  "version": "0.0.0",
  "scripts": {
    "audit": "tablecheck-scripts auditjs",
    "audit:ci": "tablecheck-scripts auditjs --ci",
    "build": "tablecheck-scripts build-lib",
    "build:storybook": "build-storybook -o docs -s assets",
    "coverage-summary": "tablecheck-scripts coverage-summary",
    "format": "prettier -w -u *",
    "postinstall": "husky install && lerna bootstrap",
    "lint": "tablecheck-scripts lint",
    "prepare": "npm run build",
    "release:auto": "lerna publish -y",
    "release:canary": "lerna publish --canary -y",
    "start": "start-storybook -s assets -p 6006",
    "test": "tablecheck-scripts test --coverage --env=jsdom",
    "test:watch": "tablecheck-scripts test --env=jsdom",
    "tsc": "tablecheck-scripts tsc",
    "tsc:watch": "tablecheck-scripts tsc --watch"
  },
  "dependencies": {
    "@auto-it/all-contributors": "10.37.1",
    "@auto-it/first-time-contributor": "10.37.1",
    "@auto-it/npm": "10.37.1",
    "@auto-it/released": "10.37.1",
    "@auto-it/slack": "10.37.1",
    "@carbon/icons-react": "11.5.0",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-lerna-scopes": "17.0.2",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-docs": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.9",
    "@storybook/addons": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/theming": "6.5.9",
    "@tablecheck/babel-preset": "1.1.0",
    "@tablecheck/eslint-config": "1.7.2",
    "@tablecheck/locales": "1.1.1",
    "@tablecheck/scripts": "2.1.0",
    "@types/enzyme": "3.10.12",
    "@types/flushable": "1.0.1",
    "@types/jest": "28.1.3",
    "@types/lodash": "4.14.182",
    "@types/raf-schd": "4.0.1",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-transition-group": "4.4.4",
    "@types/uuid": "^8.3.4",
    "ast-types": "0.14.2",
    "auditjs": "4.0.37",
    "auto": "10.37.1",
    "babel-eslint": "11.0.0-beta.0",
    "babel-preset-razzle": "4.2.17",
    "enum-for": "0.0.5",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "lerna": "5.1.4",
    "lodash": "4.17.21",
    "prettier": "2.7.1",
    "storybook": "6.5.9",
    "storybook-dark-mode": "1.1.0",
    "storybook-rtl-addon": "0.3.3",
    "typescript": "4.7.4"
  }
}
